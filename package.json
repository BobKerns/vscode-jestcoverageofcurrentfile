{
  "name": "jestcoverageofcurrentfile",
  "displayName": "JestCoverageOfCurrentFile",
  "description": "run jest for current file and get coverage info",
  "version": "0.0.1",
  "keywords": [
    "jest",
    "coverage"
  ],
  "publisher": "findxc",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Testing"
  ],
  "activationEvents": [
    "onCommand:jestcoverageofcurrentfile.run"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "jestcoverageofcurrentfile.run",
        "key": "ctrl+k j",
        "mac": "cmd+k j",
        "when": "resourceFilename =~ /\\.(spec|test)\\.[tj]sx?$/"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
